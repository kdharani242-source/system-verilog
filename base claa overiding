class parent;
  int a=20;
  function void pp();
    $display("a=%0d",a);
  endfunction
endclass
class child extends parent;
  int a=40;
  function void pp();
    $display("a=%0d",a);
  endfunction
endclass
module tb;
  child c;
  initial
    begin
      c=new();
      c.pp();
    end
endmodule
# KERNEL: a=40
