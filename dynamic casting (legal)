// Code your testbench here
// or browse Examples
class ethernet_packet;
  int a;
endclass
class good_eth_pkt extends ethernet_packet;
  int b;
endclass
module tb;
  bit y;
  ethernet_packet eth_pkt;
  good_eth_pkt g_pkt;
  initial
    begin
      eth_pkt=new();
      g_pkt=new();
      $cast(eth_pkt, g_pkt);
      y=$cast(eth_pkt, g_pkt);
      $display("y=%0d",y);
    end
endmodule
//output
Y=1
No errora or warnings are nor printed
